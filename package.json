{
  "name": "growth-stars-ui",
  "version": "0.0.1",
  "main": "lib/index.umd.min.js",
  "keyword": "growth-stars-ui vue组件 交互组件",
  "description": "繁星组件库，基于vue实现的交互组件",
  "author": "zyprepare@163.com",
  "scripts": {
    "start": "npm run serve & npm run format",
    "serve": "vue-cli-service serve",
    "pre": "vue-cli-service build --pre",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fix": "eslint --fix --ext .js --ext .vue src/**/*.{js,vue}",
    "format": "onchange 'src/**/*.{js,vue,scss}' 'example/src/**/*.{js,vue,scss}' -- prettier --write {{changed}}",
    "lib": "vue-cli-service build --target lib --name index --dest lib src/index.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "deploy": "gh-pages -d docs/.vuepress/dist"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.13.0",
    "onchange": "^6.1.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  }
}
